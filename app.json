{
  "expo": {
    "name": "rnTooltipTester",
    "slug": "RNTooltipTester",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Testing App for rn-tooltip | Serves as a QA app.",
    "githubUrl": "https://github.com/AndreiCalazans/rnTooltipTester"
  }
}
